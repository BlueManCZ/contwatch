{
  "compilerOptions": {
    "module": "es2022",
    "target": "es6",
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "moduleResolution": "Node",
    "outDir": "modules/web_server/static/js"
  },
  "include": [
    "modules/web_server/src/*"
  ],
  "exclude": [
    "webpack.config.ts"
  ]
}
